{
    "name": "somnambulist/laravel-doctrine-tenancy",
    "type": "library",
    "description": "A multi-tenancy implementation that uses Laravel and Doctrine.",
    "keywords": [
        "doctrine",
        "laravel",
        "laravel-doctrine",
        "tenancy",
        "multi-tenancy"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Redfern",
            "email": "info@somnambulist.tech"
        }
    ],
    "autoload": {
        "psr-4": {
            "Somnambulist\\Tenancy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Somnambulist\\Tenancy\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "eloquent/enumeration": "^5.1|^6.0",
        "illuminate/routing": "^5.4|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "doctrine/orm": "^2.6",
        "laravel/framework": "^5.4|^6.0|^7.0",
        "symfony/var-dumper": "^5.0",
        "rcrowe/twigbridge": "^0.11",
        "twig/extensions": "^1.5"
    },
    "suggest": {
        "rcrowe/twigbridge": "Twig integration for the Twig extension, ~0.9.*",
        "twig/extensions": "Additional useful Twig extensions ~1.3.0"
    }
}
